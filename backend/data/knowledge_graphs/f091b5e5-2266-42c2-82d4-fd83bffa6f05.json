{
  "nodes": [
    {
      "id": "root",
      "label": "微积分：理论基础与工程应用",
      "type": "root",
      "description": "数学分析的核心课程，涵盖极限、导数、积分等基础理论及其在工程中的应用。",
      "chapter_id": "id_1"
    },
    {
      "id": "mod_1",
      "label": "极限与连续性",
      "type": "module",
      "description": "研究函数极限的定义和性质，以及函数连续性的判定。",
      "chapter_id": "id_1"
    },
    {
      "id": "mod_2",
      "label": "导数与微分",
      "type": "module",
      "description": "探讨导数的定义、计算方法及微分的基本概念。",
      "chapter_id": "id_2"
    },
    {
      "id": "mod_3",
      "label": "中值定理与泰勒公式",
      "type": "module",
      "description": "研究微分中值定理及其在近似计算中的应用。",
      "chapter_id": "id_3"
    },
    {
      "id": "mod_4",
      "label": "不定积分与定积分",
      "type": "module",
      "description": "解析不定积分的基本方法及定积分的几何意义。",
      "chapter_id": "id_4"
    },
    {
      "id": "mod_5",
      "label": "积分技巧与数值方法",
      "type": "module",
      "description": "介绍换元积分法、分部积分法及数值积分算法。",
      "chapter_id": "id_5"
    },
    {
      "id": "mod_6",
      "label": "多变量微分学",
      "type": "module",
      "description": "研究多变量函数的偏导数、全微分及优化问题。",
      "chapter_id": "id_6"
    },
    {
      "id": "mod_7",
      "label": "多重积分与格林公式",
      "type": "module",
      "description": "讲解二重积分、三重积分及格林公式的应用。",
      "chapter_id": "id_7"
    },
    {
      "id": "mod_8",
      "label": "微分方程基础",
      "type": "module",
      "description": "介绍常微分方程的求解策略及其物理建模应用。",
      "chapter_id": "id_8"
    },
    {
      "id": "con_1",
      "label": "极限",
      "type": "concept",
      "description": "描述函数或序列趋于某值的趋势。",
      "chapter_id": "id_1"
    },
    {
      "id": "con_2",
      "label": "连续性",
      "type": "concept",
      "description": "函数在其定义域上无跳跃变化的性质。",
      "chapter_id": "id_1"
    },
    {
      "id": "thm_1",
      "label": "极限存在准则",
      "type": "theorem",
      "description": "夹逼定理与单调有界定理。",
      "chapter_id": "id_1"
    },
    {
      "id": "con_3",
      "label": "导数",
      "type": "concept",
      "description": "函数在某点的变化率。",
      "chapter_id": "id_2"
    },
    {
      "id": "con_4",
      "label": "微分",
      "type": "concept",
      "description": "函数增量的线性主部。",
      "chapter_id": "id_2"
    },
    {
      "id": "thm_2",
      "label": "罗尔定理",
      "type": "theorem",
      "description": "若函数在区间内可导且端点值相等，则中间必有一点导数为零。",
      "chapter_id": "id_3"
    },
    {
      "id": "thm_3",
      "label": "拉格朗日中值定理",
      "type": "theorem",
      "description": "若函数在闭区间上连续，在开区间上可导，则至少存在一点满足平均变化率等于瞬时变化率。",
      "chapter_id": "id_3"
    },
    {
      "id": "thm_4",
      "label": "泰勒定理",
      "type": "theorem",
      "description": "任意光滑函数可在某点展开为多项式形式。",
      "chapter_id": "id_3"
    },
    {
      "id": "con_5",
      "label": "不定积分",
      "type": "concept",
      "description": "原函数的集合。",
      "chapter_id": "id_4"
    },
    {
      "id": "con_6",
      "label": "定积分",
      "type": "concept",
      "description": "函数在区间上的累积效应。",
      "chapter_id": "id_4"
    },
    {
      "id": "meth_1",
      "label": "换元积分法",
      "type": "method",
      "description": "通过变量替换简化积分计算。",
      "chapter_id": "id_5"
    },
    {
      "id": "meth_2",
      "label": "分部积分法",
      "type": "method",
      "description": "将复杂积分拆分为两部分分别处理。",
      "chapter_id": "id_5"
    },
    {
      "id": "con_7",
      "label": "偏导数",
      "type": "concept",
      "description": "多变量函数对某一变量的导数。",
      "chapter_id": "id_6"
    },
    {
      "id": "con_8",
      "label": "全微分",
      "type": "concept",
      "description": "多变量函数的微小变化量的线性逼近。",
      "chapter_id": "id_6"
    },
    {
      "id": "con_9",
      "label": "二重积分",
      "type": "concept",
      "description": "在二维区域上的积分。",
      "chapter_id": "id_7"
    },
    {
      "id": "thm_5",
      "label": "格林公式",
      "type": "theorem",
      "description": "平面曲线积分与二重积分之间的转换关系。",
      "chapter_id": "id_7"
    },
    {
      "id": "con_10",
      "label": "常微分方程",
      "type": "concept",
      "description": "含有未知函数及其导数的方程。",
      "chapter_id": "id_8"
    }
  ],
